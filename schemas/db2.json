{
  "Product_DB2": {
    "type": "object",
    "properties": {
      "IDP": {
        "type": "integer"
      },
      "name": {
        "type": "string"
      },
      "brand": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "image_url": {
        "type": "string"
      },
      "price": {
        "type": "object",
        "properties": {
          "amount": {
            "type": "number"
          },
          "currency": {
            "type": "string"
          },
          "vat_rate": {
            "type": "number"
          }
        },
        "required": ["amount", "currency", "vat_rate"]
      },
      "categories": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            }
          },
          "required": ["title"]
        }
      },
      "supplier": {
        "type": "object",
        "properties": {
          "IDS": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "SIRET": {
            "type": "string"
          },
          "headOffice": {
            "type": "string"
          },
          "revenue": {
            "type": "integer"
          }
        },
        "required": ["IDS", "name", "SIRET", "headOffice", "revenue"]
      },
      "stocks": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "IDW": {
              "type": "integer"
            },
            "quantity": {
              "type": "integer"
            },
            "location": {
              "type": "string"
            }
          },
          "required": ["IDW", "quantity", "location"]
        }
      }
    },
    "required": ["IDP", "name", "brand", "description", "image_url", "price", "categories", "supplier", "stocks"]
  },
  "Warehouse_DB2": {
    "type": "object",
    "properties": {
      "IDW": {
        "type": "integer"
      },
      "address": {
        "type": "string"
      },
      "capacity": {
        "type": "integer"
      }
    },
    "required": ["IDW", "address", "capacity"]
  },
  "OrderLine_DB2": {
    "type": "object",
    "properties": {
      "IDC": {
        "type": "integer"
      },
      "IDP": {
        "type": "integer"
      },
      "date": {
        "type": "string",
        "format": "date"
      },
      "quantity": {
        "type": "integer"
      },
      "deliveryDate": {
        "type": "string",
        "format": "date"
      },
      "comment": {
        "type": "string"
      },
      "grade": {
        "type": "integer"
      }
    },
    "required": ["IDC", "IDP", "date", "quantity", "deliveryDate", "comment", "grade"]
  },
  "Client_DB2": {
    "type": "object",
    "properties": {
      "IDC": {
        "type": "integer"
      },
      "ln": {
        "type": "string"
      },
      "fn": {
        "type": "string"
      },
      "address": {
        "type": "string"
      },
      "nationality": {
        "type": "string"
      },
      "birthDate": {
        "type": "string",
        "format": "date"
      },
      "email": {
        "type": "string"
      }
    },
    "required": ["IDC", "ln", "fn", "address", "nationality", "birthDate", "email"]
  }
}
